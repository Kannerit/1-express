  <div class="container mt-5">
    <h1>User Table</h1>
    <table class="table">
      <thead>
        <tr>
          <th>Name</th>
          <th>Email</th>
          <th>Address</th>
          <th>Website</th>
        </tr>
      </thead>
      <tbody>
        {{#each users}}
        <tr>
          <td>{{this}}</td>
          <td>{{user.email}}</td>
          <td>{{this.address.street}}, {{this.address.suite}}, {{this.address.city}}, {{this.address.zipcode}}</td>
          <td><a href="{{this.website}}" target="_blank">{{this.website}}</a></td>
        </tr>
        {{/each}}
      </tbody>
    </table>
  </div>